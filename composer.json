{
    "name": "dgtlss/scribe",
    "description": "An easy-to-use documentation package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Langer",
            "email": "nlangerdev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "league/commonmark": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Dgtlss\\Scribe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dgtlss\\Scribe\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dgtlss\\Scribe\\ScribeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}